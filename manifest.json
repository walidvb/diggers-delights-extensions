{
  "manifest_version": 2,
  "name": "DiggersDelights",
  "description": "The easiest way to dig songs and share them to your friends!",
  "version": "1",
  "author": "walidvb",
  "browser_action": {
    "default_icon": "logo.svg",
    "default_title": "DiggersDelights",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "all_frames": true,
    "js": [
      "iframe.js"
    ]
  }],
  "permissions": ["activeTab"]
}